 <%= render 'shared_pages/page_title', title: "Get in touch with us" %>
 
 <!-- Main content -->
    <section class="container tm-contact-main">
      <div class="row">
        <div id="containers">
          
              <div class="map ">
      <div class="strip info">
          <h3 id="ask" style="text-align: center;">Find where we are based</h3>
      <div style="width: 100%">
        <iframe width="100%" height="400" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" 
          src="https://maps.google.com/maps?width=100%25&amp;height=400&amp;hl=en&amp;q=blandchardstown+(Food%20Universum)&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed" 
          style="width: inherit;">
        </iframe>
    </div>
        </div>
      </div> <!-- end map-->
      <div class="row">
        <div class="contact-form-container">
          <h2 class="contact-title">Contact Us</h2>
          <p>Send us a message, we will come back to you as soon as possible. The tips and information that you provide us will be used in order to improve our page.</p>
          <form action="#" method="post" class="tm-contact-form">
              <div class="col-lg-5 col-md-5 contact-form-left">
                <div class="form-group">
                    <input class="form-control" type="text" name="" value="" placeholder="Enter Name">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="" value="" placeholder="Enter Email">
                </div>
                <div class="form-group">
                    <input type="text" id="contact_subject" class="form-control" placeholder="SUBJECT" />
                </div>
              </div>
              <div class="col-lg-7 col-md-7 contact-form-right">
                <div class="form-group margin-bottom-0">
                    <textarea id="contact_message" class="form-control" rows="6" placeholder="MESSAGE"></textarea>
                </div>
              </div>
              <div class="col-lg-12 col-md-12 submit-btn-container">
                <button class="btn btn-default">Submit</button>
              </div>
          </form>
        </div>
      </div>
      
          <section id="contact">
      <div class="container">
        <div class="row">
          <div class="col-md-5 col-sm-5">
            <form>
              <div class="form-group">
                <label>Name: </label>
                <input class="form-control" type="text" name="" value="" placeholder="Enter Name" required=true>
              </div>
              <div class="form-group">
                <label>Email: </label>
                <input class="form-control" type="text" name="" value="" placeholder="Enter Email">
              </div>
              <div class="form-group">
                <label>Message: </label>
                <textarea class="form-control" name="" value="" placeholder="Enter Message"></textarea>
              </div>
              <button class="btn btn-default">Submit</button>
            </form>
          </div>
          <div class="col-md-7 col-sm-7">

          </div>
        </div>
      </div>
    </section>
      <!--banner-->
    <%= render 'shared_pages/banner_sign_up', title: "sign up banner" %>
    </section>

    <!-- End Main content -->